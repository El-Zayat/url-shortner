<div id="alerts" class="my-3">
    <% for(let error of flashed.errors) { %>
    <div class="alert alert-danger"><%= error %></div>
    <% } %> <% for(let warning of flashed.warnings) { %>
    <div class="alert alert-warning"><%= warning %></div>
    <% } %> <% for(let horraaayy of flashed.successes) { %>
    <div class="alert alert-success"><%= horraaayy %></div>
    <% } %>
</div>
